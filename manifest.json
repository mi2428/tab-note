{
  "manifest_version": 2,
  "name": "Tab Note",
  "version": "1.0",
  "description": "Simple note-taking extension for Numix Theme",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "content_security_policy": "script-src 'self' https://code.jquery.com; object-src 'self'",
  "permissions": [
    "tabs",
    "storage"
  ],
  "icons": {
    "128": "icon/128.png"
  }
}
